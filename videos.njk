---
layout: layouts/home.njk
eleventyNavigation:
  key: Contacto
  order: 4
  url: /#contact
---

<!-- Grid for photos -->
<div class="container">

  <style>
    .row {
      display: flex;
      flex-wrap: wrap;
      padding: 0 4px;
    }

    /* Create two equal columns that sit next to each other */
    .column {
      flex: 50%;
      max-width: 50%;
      padding: 0 4px;
    }

    .column img {
      margin-top: 8px;
      vertical-align: middle;
    }
    {% raw %}
      @media (max-width: 600px) {
        .column {
          flex: 100%;
          max-width: 100%;
        }
      {% endraw %}
  </style>
  <div class="row">

    {% for post in collections.videos %}

      <div class="col-lg-6 mb-4">

        {# <a href="{{ post.url | url}}"><img src = "{{ post.data.image | url }}" style="width:100%;margin-bottom:0px" > </a> #}
        <p>{{ post.data.videourl }}</p>

      </div>
    {% endfor %}
  </div>

  <!-- End photo grid -->
</div>
