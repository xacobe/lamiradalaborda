{# Pagination #}
    <nav aria-labelledby="paginador" class="position-relative pb-3 d-flex justify-content-center">
      <h2 id="my-pagination" class="visually-hidden">Paginaci√≥n</h2>
      <ol class="pagination">
        {% if pagination.href.previous %}
          <li class="page-item">
            <a class="page-link" href="{{ pagination.href.previous }}"><</a>
          </li>
            {% endif %}
          {%- for pageEntry in pagination.pages %}
            <li class="page-item">
              <a class="page-link" href="{{ pagination.hrefs[ loop.index0 ] }}"{% if page.url == pagination.hrefs[ loop.index0 ] %} aria-current="page"{% endif %}>{{ loop.index }}</a>
            </li>
          {%- endfor %}
            {% if pagination.href.next %}
          <li class="page-item">
              <a class="page-link" href="{{ pagination.href.next }}">></a>
          </li>
        {% endif %}
      </ol>
    </nav>
