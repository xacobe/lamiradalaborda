<div id="carouselExampleControls" class="carousel slide p-0" data-bs-ride="carousel">
  <div class="carousel-inner">
    {% for item in pagination.items | head(1, 1) %}
      <div class="carousel-item active">
        <div class="col-lg-12 mb-4">
          {% include 'youtube.njk' %}
        </div>
      </div>
    {% endfor %}
    {# {% for post in collections.videos | head(-2) %} #}
    {% for item in pagination.items | head(-3, -5) %}
      <div class="carousel-item">
        <div class="col-lg-12 mb-4">
          {% include 'youtube.njk' %}
        </div>
      </div>
    {% endfor %}
  </div>
  <button class="carousel-control-prev float-start float-lg-none" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
    <span class="carousel-control-prev-icon p-4 p" aria-hidden="true"></span>
    <span class="visually-hidden">Anterior</span>
  </button>
  <button class="carousel-control-next float-start float-lg-none" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
    <span class="carousel-control-next-icon p-4" aria-hidden="true"></span>
    <span class="visually-hidden">Siguiente</span>
  </button>
</div>
